{
  "name": "sep",
  "author": "Kevin Bravo (kevinbravo.me)",
  "description": "Sistema para evaluar el progreso del becario durante su estadia en el Programa Excelencia",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/mustafinho/seb",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0",
    "npm": "please-use-pnpm"
  },
  "prisma": {
    "schema": "src/lib/db/prisma/schema.prisma"
  },
  "scripts": {
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:delete": "rm 'node_modules/.pnpm/@prisma+client@5.1.0_prisma@5.1.0/node_modules/.prisma/client/query_engine-windows.dll.node'",
    "prisma:generate": "pnpm run prisma:import && prisma generate --schema=./src/lib/db/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/lib/db/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/lib/db/prisma/schema.prisma",
    "prisma:push": "prisma db push dev --schema=./src/lib/db/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./src/lib/db/prisma/schema.prisma",
    "prisma:import": "rm ./src/lib/db/prisma/schema.prisma && pnpm prisma-import --schemas=./src/lib/db/prisma/**/*.prisma --output=./src/lib/db/prisma/schema.prisma && pnpm prisma:format"
  },
  "dependencies": {
    "@googleapis/calendar": "^4.1.0",
    "@googleapis/drive": "^5.0.2",
    "@googleapis/forms": "^2.0.0",
    "@googleapis/gmail": "^1.2.0",
    "@googleapis/oauth2": "^1.0.0",
    "@googleapis/people": "^3.0.2",
    "@googleapis/sheets": "^4.0.2",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "apexcharts": "^3.42.0",
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "fs": "0.0.1-security",
    "jotai": "^2.0.4",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "next-auth-prisma-adapter": "^0.1.0",
    "next-themes": "^0.2.1",
    "prisma": "^5.1.0",
    "prisma-import": "^1.0.4",
    "react": "18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-big-calendar": "^1.6.9",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-table": "^7.8.0",
    "short-uuid": "^4.2.2",
    "swr": "^2.1.5",
    "tailwindcss": "3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@microsoft/microsoft-graph-types": "^2.33.0",
    "@microsoft/microsoft-graph-types-beta": "0.35.0-preview",
    "@tailwindcss/forms": "^0.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/microsoft-graph": "^2.32.0",
    "@types/node": "18.15.11",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "18.0.33",
    "@types/react-big-calendar": "^1.6.3",
    "@types/react-dom": "18.0.11",
    "@types/react-table": "^7.7.14",
    "autoprefixer": "10.4.14",
    "eslint": "8.37.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "nodemailer": "^6.9.3",
    "postcss": "8.4.21",
    "prettier": "^3.0.1",
    "typescript": "5.0.3"
  }
}
