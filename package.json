{
  "name": "sep",
  "author": "Kevin Bravo (@_bravok)",
  "description": "Sistema para evaluar el progreso del becario durante su estadia en el Programa Excelencia",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/mustafinho/seb",
  "engines": {
    "node": ">=16.6.0",
    "pnpm": ">=7.27.0",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:delete": "rm ./node_modules/.pnpm/@prisma+client@4.15.0_prisma@4.15.0/node_modules/.prisma/client/query_engine-windows.dll.node",
    "prisma:generate": "pnpm run prisma:import && prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "prisma:push": "prisma db push dev --schema=./src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./src/prisma/schema.prisma",
    "prisma:import": "rm ./src/prisma/schema.prisma && pnpm prisma-import --schemas=./src/prisma/**/*.prisma --output=./src/prisma/schema.prisma && pnpm prisma:format"
  },
  "dependencies": {
    "@googleapis/calendar": "^4.1.0",
    "@googleapis/drive": "^5.0.2",
    "@googleapis/forms": "^2.0.0",
    "@googleapis/gmail": "^1.2.0",
    "@googleapis/oauth2": "^1.0.0",
    "@googleapis/people": "^3.0.2",
    "@googleapis/sheets": "^4.0.2",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.15.0",
    "autoprefixer": "10.4.14",
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "eslint": "8.37.0",
    "eslint-config-next": "13.3.0",
    "fs": "0.0.1-security",
    "jotai": "^2.0.4",
    "moment": "^2.29.4",
    "next": "13.4.5",
    "next-auth": "4.22.1",
    "postcss": "8.4.21",
    "prisma": "^4.15.0",
    "prisma-import": "^1.0.4",
    "react": "18.2.0",
    "react-big-calendar": "^1.6.9",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-table": "^7.8.0",
    "short-uuid": "^4.2.2",
    "swr": "^2.1.5",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.3"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^2.33.0",
    "@microsoft/microsoft-graph-types-beta": "0.35.0-preview",
    "@tailwindcss/forms": "^0.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/microsoft-graph": "^2.32.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-big-calendar": "^1.6.3",
    "@types/react-dom": "18.0.11",
    "@types/react-table": "^7.7.14"
  }
}